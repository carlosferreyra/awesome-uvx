      "execs": [
        "csvclean",
        "csvcut",
        "csvformat",
        "csvgrep",
        "csvjoin",
        "csvjson",
        "csvlook",
        "csvpy",
        "csvsort",
        "csvsql",
        "csvstack",
        "csvstat",
        "in2csv",
        "sql2csv"
      ]
    },
    "easyocr": {
      "description": "Ready-to-use OCR with 80+ languages supported",
